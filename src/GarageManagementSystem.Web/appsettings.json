{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "IdentityServer": {
    "Authority": "https://localhost:44333",
    "ClientId": "garage.web",
    "ClientSecret": "garage.web.secret",
    "ResponseType": "code",
    "Scopes": [
      "openid",
      "profile",
      "garage.api"
    ],
    "SaveTokens": true,
    "GetClaimsFromUserInfoEndpoint": true,
    "RequireHttpsMetadata": false,
    "SignedOutRedirectUri": "/Home/Index",
    "SignedOutCallbackPath": "/signout-callback-oidc",
    "BackchannelTimeout": 30
  },
  "ApiConfiguration": {
    "BaseUrl": "https://localhost:44303/api/",
    "TimeoutSeconds": 30
  }
}
